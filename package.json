  },
  "homepage": "https://github.com/Hatkom-io/prisma-generator-c-sharp",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hatkom-io/prisma-generator-c-sharp"
  },
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
